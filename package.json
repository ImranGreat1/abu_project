{
  "name": "Edutain",
  "version": "1.0.0",
  "description": "Edutain, an app that give ABU students the information they need",
  "main": "app.js",
  "scripts": {
    "server": "nodemon server.js",
    "client": "cd client && npm start",
    "start": "concurrently \"npm run server\" \"npm run client\" ",
